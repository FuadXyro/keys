										href="/users/logout">
										<i class="w-4 h-4 mr-3 fa-solid fa-right-from-bracket"></i>
										<span> Log out</span>
									</a>
								</li>
								<% } else { %>
								<li class="flex">
									<a class="inline-flex items-center w-full px-2 py-1 text-sm font-semibold transition-colors duration-150 rounded-md hover:bg-gray-100 hover:text-gray-800 dark:hover:bg-gray-800 dark:hover:text-gray-200"
										href="/users/login">
										<i class="w-4 h-4 mr-3 fa-solid fa-right-to-bracket"></i>
										<span> Login</span>
									</a>
								</li>
								<li class="flex">
									<a class="inline-flex items-center w-full px-2 py-1 text-sm font-semibold transition-colors duration-150 rounded-md hover:bg-gray-100 hover:text-gray-800 dark:hover:bg-gray-800 dark:hover:text-gray-200"
										href="/users/register">
										<i class="w-4 h-4 mr-3 fa-solid fa-registered"></i>
										<span> Registered</span>
									</a>
								</li>
								<li class="flex">
									<a class="inline-flex items-center w-full px-2 py-1 text-sm font-semibold transition-colors duration-150 rounded-md hover:bg-gray-100 hover:text-gray-800 dark:hover:bg-gray-800 dark:hover:text-gray-200"
										href="/users/forgotpassword">
										<i class="w-4 h-4 mr-3 fa-brands fa-keycdn"></i>
										<span> Forgot Password</span>
									</a>
								</li>
								<% } %> 
								
							</ul>
						</template>
					</li>
				</ul>
			</div>
		</header>
