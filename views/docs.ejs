<%- include("./layouts/list-ft.ejs") %>
	<div class="flex flex-col flex-1 w-full">
		<header class="z-10 py-4 bg-white shadow-md dark:bg-gray-800">
			<div class="container flex items-center justify-between h-full px-6 mx-auto text-red-600 dark:text-red-300">
				<!-- Mobile hamburger -->
				<button class="p-1 mr-5 -ml-1 rounded-md md:hidden focus:outline-none focus:shadow-outline-red"
					@click="toggleSideMenu" aria-label="Menu">
					<i class="fa-solid fa-bars-staggered"></i>
				</button>
				<!-- Search input -->
				<div class="flex justify-center flex-1 lg:mr-32">
					<div class="relative w-full max-w-xl mr-6 focus-within:text-red-500">
						<div class="absolute inset-y-0 flex items-center pl-2">
							<i class="fa-solid fa-magnifying-glass"></i>
						</div>
						<input
							class="w-full pl-8 pr-2 text-sm text-center text-gray-700 placeholder-red-600 bg-red-100 border-0 rounded-md dark:placeholder-red-500 dark:focus:shadow-outline-gray dark:focus:placeholder-gray-600 dark:bg-gray-700 dark:text-gray-200 focus:placeholder-gray-500 focus:bg-white focus:border-red-300 focus:outline-none focus:shadow-outline-red form-input"
							type="text" placeholder="Searching......." aria-label="Search" />
					</div>
				</div>
				<ul class="flex items-center flex-shrink-0 space-x-6">
					<!-- Theme toggler -->
					<li class="flex">
						<button class="rounded-md focus:outline-none focus:shadow-outline-red" @click="toggleTheme"
							aria-label="Toggle color mode">
							<template x-if="!dark">
								<svg class="w-5 h-5" aria-hidden="true" fill="currentColor" viewBox="0 0 20 20">
									<path d="M17.293 13.293A8 8 0 016.707 2.707a8.001 8.001 0 1010.586 10.586z"></path>
								</svg>
							</template>
							<template x-if="dark">
								<svg class="w-5 h-5" aria-hidden="true" fill="currentColor" viewBox="0 0 20 20">
									<path fill-rule="evenodd"
										d="M10 2a1 1 0 011 1v1a1 1 0 11-2 0V3a1 1 0 011-1zm4 8a4 4 0 11-8 0 4 4 0 018 0zm-.464 4.95l.707.707a1 1 0 001.414-1.414l-.707-.707a1 1 0 00-1.414 1.414zm2.12-10.607a1 1 0 010 1.414l-.706.707a1 1 0 11-1.414-1.414l.707-.707a1 1 0 011.414 0zM17 11a1 1 0 100-2h-1a1 1 0 100 2h1zm-7 4a1 1 0 011 1v1a1 1 0 11-2 0v-1a1 1 0 011-1zM5.05 6.464A1 1 0 106.465 5.05l-.708-.707a1 1 0 00-1.414 1.414l.707.707zm1.414 8.486l-.707.707a1 1 0 01-1.414-1.414l.707-.707a1 1 0 011.414 1.414zM4 11a1 1 0 100-2H3a1 1 0 000 2h1z"
										clip-rule="evenodd"></path>
								</svg>
							</template>
						</button>
					</li>
					<!-- Profile menu -->
					<li class="relative">
						<button class="align-middle rounded-full focus:shadow-outline-red focus:outline-none"
							@click="toggleProfileMenu" @keydown.escape="closeProfileMenu" aria-label="Account"
							aria-haspopup="true">
							<img class="object-cover w-8 h-8 rounded-full" src="
					
					<%= profile %>" alt="" aria-hidden="true" />
						</button>
						<template x-if="isProfileMenuOpen">
							<ul x-transition:leave="transition ease-in duration-150"
								x-transition:leave-start="opacity-100" x-transition:leave-end="opacity-0"
								@click.away="closeProfileMenu" @keydown.escape="closeProfileMenu"
								class="absolute right-0 w-56 p-2 mt-2 space-y-2 text-gray-600 bg-white border border-gray-100 rounded-md shadow-md dark:border-gray-700 dark:text-gray-300 dark:bg-gray-700"
								aria-label="submenu">
								<% if (user) { %>
								<li class="flex">
									<a class="inline-flex items-center w-full px-2 py-1 text-sm font-semibold transition-colors duration-150 rounded-md hover:bg-gray-100 hover:text-gray-800 dark:hover:bg-gray-800 dark:hover:text-gray-200"
										href="/users/profile">
										<i class="w-4 h-4 mr-3 fa-solid fa-user-gear"></i>
										<span> Profile</span>
									</a>
								</li>
								<li class="flex">
									<a class="inline-flex items-center w-full px-2 py-1 text-sm font-semibold transition-colors duration-150 rounded-md hover:bg-gray-100 hover:text-gray-800 dark:hover:bg-gray-800 dark:hover:text-gray-200"
										href="/users/settings">
										<i class="w-4 h-4 mr-3 fa-solid fa-gears"></i>
										<span> Settings</span>
									</a>
								</li>
								<li class="flex">
									<a class="inline-flex items-center w-full px-2 py-1 text-sm font-semibold transition-colors duration-150 rounded-md hover:bg-gray-100 hover:text-gray-800 dark:hover:bg-gray-800 dark:hover:text-gray-200"
										href="/users/logout">
										<i class="w-4 h-4 mr-3 fa-solid fa-right-from-bracket"></i>
										<span> Log out</span>
									</a>
								</li>
								<% } else { %>
								<li class="flex">
									<a class="inline-flex items-center w-full px-2 py-1 text-sm font-semibold transition-colors duration-150 rounded-md hover:bg-gray-100 hover:text-gray-800 dark:hover:bg-gray-800 dark:hover:text-gray-200"
										href="/users/login">
										<i class="w-4 h-4 mr-3 fa-solid fa-right-to-bracket"></i>
										<span> Login</span>
									</a>
								</li>
								<li class="flex">
									<a class="inline-flex items-center w-full px-2 py-1 text-sm font-semibold transition-colors duration-150 rounded-md hover:bg-gray-100 hover:text-gray-800 dark:hover:bg-gray-800 dark:hover:text-gray-200"
										href="/users/register">
										<i class="w-4 h-4 mr-3 fa-solid fa-registered"></i>
										<span> Registered</span>
									</a>
								</li>
								<li class="flex">
									<a class="inline-flex items-center w-full px-2 py-1 text-sm font-semibold transition-colors duration-150 rounded-md hover:bg-gray-100 hover:text-gray-800 dark:hover:bg-gray-800 dark:hover:text-gray-200"
										href="/users/forgotpassword">
										<i class="w-4 h-4 mr-3 fa-brands fa-keycdn"></i>
										<span> Forgot Password</span>
									</a>
								</li>
								<% } %> 
								
							</ul>
						</template>
					</li>
				</ul>
			</div>
		</header>
		<main class="h-full overflow-y-auto">
			<div class="container px-6 mx-auto grid">
				<center>
					<img class="my-6" width="350px"
						src="http://readme-typing-svg.herokuapp.com?size=28&font=Caveat&color=%23818187&center=true&vCenter=true&lines=<%= text_running %>">
				</center>
				<!-- Cards -->
				<div class="grid gap-6 mb-8 md:grid-cols-2 xl:grid-cols-4">
					
					<!-- Card -->
					<div class="flex items-center p-4 bg-white rounded-lg shadow-xs dark:bg-gray-800">
						<div
							class="p-3 mr-4 text-teal-500 bg-teal-100 rounded-full dark:text-teal-100 dark:bg-teal-500">
							<i class="fa-solid fa-eye-low-vision"></i>
						</div>
						<div>
							<p class="mb-2 text-sm font-medium text-gray-600 dark:text-gray-400"> Visitors </p>
							<p class="text-lg font-semibold text-gray-700 dark:text-gray-200">
								<span id="visitor"></span> Visit
							</p>
						</div>
					</div>
					<!-- Card -->
					
					<!-- Card -->
					<div class="flex items-center p-4 bg-white rounded-lg shadow-xs dark:bg-gray-800">
						<div
							class="p-3 mr-4 text-green-500 bg-green-100 rounded-full dark:text-green-100 dark:bg-green-500">
							<i class="fa-solid fa-globe"></i>
						</div>
						<div>
							<p class="mb-2 text-sm font-medium text-gray-600 dark:text-gray-400"> Your IP Address </p>
							<p class="text-lg font-semibold text-gray-700 dark:text-gray-200">
								<span id="ip"></span>
							</p>
						</div>
					</div>
					<!-- Card -->
					<div class="flex items-center p-4 bg-white rounded-lg shadow-xs dark:bg-gray-800">
						<div
							class="p-3 mr-4 text-teal-500 bg-teal-100 rounded-full dark:text-teal-100 dark:bg-blue-500">
							<i class="fa-solid fa-battery-full"></i>
						</div>
						<div><% if (/(Android|\?1)/.test(androUser)) { %>
							<p class="mb-2 text-sm font-medium text-gray-600 dark:text-gray-400"> Your Battery </p>
							<p class="text-lg font-semibold text-gray-700 dark:text-gray-200">
								<span id="batteryLevel"></span>
								<span id="percentageLevel">100%</span>
								<span color="red" id="isCharge"></span>
							</p><% } else { %>
							<p class="mb-2 text-sm font-medium text-gray-600 dark:text-gray-400"> Your Battery
							</p>
							<p class="text-lg font-semibold text-gray-700 dark:text-gray-200">
								Only Android Users
							</p>
						<% } %></div>
					</div>
					<!-- Card -->
					<div class="flex items-center p-4 bg-white rounded-lg shadow-xs dark:bg-gray-800">
						<div class="p-3 mr-4 text-red-500 bg-red-100 rounded-full dark:text-red-100 dark:bg-orange-500">
							<i class="fa-solid fa-clock"></i>
						</div>
						<div>
							<p class="mb-2 text-sm font-medium text-gray-600 dark:text-gray-400"> Clock </p>
							<p class="text-lg font-semibold text-gray-700 dark:text-gray-200">
								<span id="Clock"></span>, <span id="Ucapan"></span>
							</p>
						</div>
					</div>
					<!-- Card -->
					<div class="flex items-center p-4 bg-white rounded-lg shadow-xs dark:bg-gray-800">
						<div
							class="p-3 mr-4 text-teal-500 bg-teal-100 rounded-full dark:text-teal-100 dark:bg-teal-500">
							<i class="fa-solid fa-hands-praying"></i>
						</div>
						<div>
							<p class="mb-2 text-sm font-medium text-gray-600 dark:text-gray-400"> Your Providers </p>
							<p class="text-lg font-semibold text-gray-700 dark:text-gray-200">
								<span id="card"></span>
							</p>
						</div>
					</div>
					<!-- Card -->
					<div class="flex items-center p-4 bg-white rounded-lg shadow-xs dark:bg-gray-800">
						<div
							class="p-3 mr-4 text-green-500 bg-green-100 rounded-full dark:text-green-100 dark:bg-green-500">
							<i class="fa-regular fa-calendar-days"></i>
						</div>
						<div>
							<p class="mb-2 text-sm font-medium text-gray-600 dark:text-gray-400"> Date </p>
							<p class="text-lg font-semibold text-gray-700 dark:text-gray-200">
								<span id="tanggal"></span>
							</p>
						</div>
					</div>
					<!-- Card -->
					<div class="flex items-center p-4 bg-white rounded-lg shadow-xs dark:bg-gray-800">
						<div
							class="p-3 mr-4 text-blue-500 bg-blue-100 rounded-full dark:text-blue-100 dark:bg-blue-500">
							<i class="fa-solid fa-stopwatch"></i>
						</div>
						<div>
							<p class="mb-2 text-sm font-medium text-gray-600 dark:text-gray-400"> Road To Eid Mubarak
							</p>
							<p class="text-lg font-semibold text-gray-700 dark:text-gray-200">
								<span id="countDown"></span>
							</p>
						</div>
					</div>
				</div>
				<!-- Cek Apikey -->
				<div class="grid gap-6 mb-8">
					<div class="min-w-0 p-4 bg-white rounded-lg shadow-xs dark:bg-gray-800">
					<form action="/api/cekkey?apikey= apikey %"
						<label class="block text-sm text-center">
							<p class="status font-bold text-gray-700 dark:text-gray-400"> Cek Your Apikey <i
									class="fa-brands fa-keycdn"></i>
							</p>
							<br>
							<hr class="mb-8">
							<input type="text" id="input-apikey"
								class="text-center block w-full mt-1 text-sm dark:border-gray-600 dark:bg-gray-700 focus:border-red-400 focus:outline-none focus:shadow-outline-red dark:text-gray-300 dark:focus:shadow-outline-gray form-input"
								placeholder="<%= typeof apikey == " APIKEY" ? 'Input Your Apikey' : apikey %>" required>
						</label>
						<button id="submit-apikey"
							class="block w-full px-4 py-2 mt-4 text-sm font-medium leading-5 text-center text-white transition-colors duration-150 bg-red-600 border border-transparent rounded-lg active:bg-red-600 hover:bg-red-700 focus:outline-none focus:shadow-outline-red"
							type="submit" onclick="execute()"> Submit</button>
					</div>
					<!-- Cover base -->
					<div class="grid gap-6 mb-8">
						<div class="min-w-0 p-4 bg-white justify-center rounded-lg shadow-xs dark:bg-gray-800">
							<center>
								<img aria-hidden="true" class="hidden rounded-md object-cover w-full h-full dark:block"
									src="images/docs-dark.png">
								<img aria-hidden="true"
									class="object-cover rounded-md object-cover w-full h-full dark:hidden"
									src="images/docs-light.png">
							</center>
							<div class="flex justify-center mt-4 space-x-3 text-sm text-gray-600 dark:text-red-400">
								<!-- Team -->
								<div class="flex items-center">
									<span class="inline-block w-3 h-3 mr-1 bg-teal-600 rounded-md"></span>
									<span> ArifzynXD TEAM</span>&nbsp; <span
										class="inline-block w-3 h-3 mr-1 bg-blue-500 rounded-md"></span>
								</div>
							</div>
							<hr class="my-6">
							<p class="text-sm text-center text-gray-600 font-bold dark:text-red-400">
								<span class="underline"> • ANNOUNCEMENT • </span>
							</p>
							<br>
							<span class="text-sm text-gray-600 font-semibold dark:text-white">• Harap gunakan rest api sebijak mungkin jangan meniru, mengcopy template website
								. <br>• Kalo ada bug harap segera lapor. <br>•
								Terima kasih
								perhatiannya. </span>
						</div>
						<footer>
							<div class="grid gap-6 mb-10">
								<div class="min-w-0 p-4 bg-white rounded-lg shadow-xs dark:bg-gray-800">
									<p class="text-center text-sm text-gray-600 font-bold dark:text-gray-400"> Powered
										By <a href="https://github.com/wffzy">ArifzynXD</a> <span class="text-red-600"
											id="years"></span>
									</p>
								</div>
							</div>
						</footer>
					</div>
				</div>
		</main>
	</div>
</div>